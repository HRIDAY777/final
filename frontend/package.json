{
  "name": "educore-ultra-frontend",
  "version": "1.0.0",
  "description": "AI-Powered School Management System Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "build:prod": "./scripts/build-production.sh",
    "preview": "vite preview --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest --run",
    "test:all": "./scripts/test.sh",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "deploy": "./scripts/deploy.sh",
    "deploy:local": "DEPLOY_ENV=local ./scripts/deploy.sh",
    "deploy:staging": "DEPLOY_ENV=staging ./scripts/deploy.sh",
    "deploy:production": "DEPLOY_ENV=production ./scripts/deploy.sh",
    "analyze": "ANALYZE_BUNDLE=true npm run build:prod",
    "clean": "rm -rf dist node_modules/.vite coverage test-results",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npm update",
    "deps:outdated": "npm outdated",
    "precommit": "npm run lint && npm run type-check && npm run test:run",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@react-oauth/google": "^0.12.2",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.16",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "react-table": "^7.8.0",
    "react-virtualized": "^9.22.5",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.3",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^1.3.0",
    "@vitest/coverage-v8": "^1.3.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "cssnano": "^7.0.6",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.90.0",
    "typescript": "^5.2.2",
    "vite": "^6.1.6",
    "vite-plugin-compression2": "^1.3.0",
    "vite-plugin-pwa": "^0.21.0",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
