# EduCore Ultra Frontend - Production Environment

# API Configuration
VITE_API_BASE_URL=https://yourdomain.com/api
VITE_WS_BASE_URL=wss://yourdomain.com/ws

# Application Configuration
VITE_APP_NAME=EduCore Ultra
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered School Management System

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=CHANGE_ME_GOOGLE_ANALYTICS_ID
VITE_SENTRY_DSN=CHANGE_ME_SENTRY_DSN

# Payment Gateway
VITE_STRIPE_PUBLISHABLE_KEY=CHANGE_ME_STRIPE_PUBLISHABLE_KEY

# Social Authentication
VITE_GOOGLE_OAUTH2_CLIENT_ID=CHANGE_ME_GOOGLE_OAUTH2_CLIENT_ID
VITE_FACEBOOK_APP_ID=CHANGE_ME_FACEBOOK_APP_ID

# AI Services
VITE_OPENAI_API_KEY=CHANGE_ME_OPENAI_API_KEY
VITE_ANTHROPIC_API_KEY=CHANGE_ME_ANTHROPIC_API_KEY

# Monitoring
VITE_ENABLE_MONITORING=true
VITE_MONITORING_ENDPOINT=https://yourdomain.com/monitoring

# Development Tools (Disabled in Production)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEV_TOOLS=false
